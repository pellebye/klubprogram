package _start.kalender;

import common.LocalMethods;
import common.log.CommonLog;
import common.out.info.InfoWeekConflict;

public class MinusWeekList {

	private int year = 0;

	public int getYear() {
		return year;
	}

	private int week = 0;

	public int getWeek() {
		return week;
	}

	private String[] minusWeekText = null;

	public String[] getMinusWeekText() {
		return minusWeekText;
	}

	// ---------------------------------------------------------------------------------------------
	// CONSTRUCTOR
	// ---------------------------------------------------------------------------------------------
	public MinusWeekList(int year, int week, String text) {
		CommonLog.logger.info("heading//");

		this.year = year;
		this.week = week;

		// Split week text into words.
		minusWeekText = text.trim().split(" ");

		// Check for any text.
		if (minusWeekText.length == 0 || minusWeekText == null)
			new InfoWeekConflict(line);

		if (LocalMethods.isParsableToInt(ugeSplit[0]))
			list[0] = Integer.parseInt(ugeSplit[0]);
	}
}
